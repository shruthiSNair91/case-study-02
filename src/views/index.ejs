                    <div class="tm-gallery">
                        <div class="row">
                            <figure class="col-lg-3 col-md-4 col-sm-6 col-12 tm-gallery-item">
                                <a href="/books">
                                    <div class="tm-gallery-item-overlay">
                                        <img src="/images/apj.jpeg" style="min-width: 65px; min-height: 360px; " alt="Image" class="img-fluid tm-img-center">
                                    </div>
                                    
                                    <p class="tm-figcaption">Wings Of Fire</p>
                                </a>
                            </figure>
                            <figure class="col-lg-3 col-md-4 col-sm-6 col-12 tm-gallery-item">
                                <a href="/books">
                                    <div class="tm-gallery-item-overlay">
                                        <img src="/images/believe_in_urself.jpg" alt="Image" class="img-fluid tm-img-center">
                                    </div>
                                    <p class="tm-figcaption">Believe in yourself</p>
                                </a>
                            </figure>
                            <figure class="col-lg-3 col-md-4 col-sm-6 col-12 tm-gallery-item">
                                <a href="/books">
                                    <div class="tm-gallery-item-overlay">
